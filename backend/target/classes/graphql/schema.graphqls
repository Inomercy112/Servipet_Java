type Producto {
    idDto : ID!
    imagenProductoDto: String!
    nombreProductoDto: String!
    descripcionProductoDto: String!
    precioProductoDto: Int!
    cantidadProductoDto:Int!
    categoriasNombresDto: [String!]!
}

type Query {
    getproductoById(id: ID!) : Producto
    getproductos: [Producto!]!
}
